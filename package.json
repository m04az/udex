{
  "name": "udex",
  "version": "1.0.0",
  "description": "A quality-of-life Extension for Udacity Reviewers.",
  "scripts": {
    "build": "parcel build src/js/main.js -d build/ -o main",
    "watch": "parcel watch src/js/main.js -d build/ -o main",
    "build:zip": "rm udex.zip && zip -r udex.zip build images manifest.json"
  },
  "author": "Mikkel Trolle Larsen",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.1",
    "styled-jsx": "^2.2.6"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.7.1"
  }
}