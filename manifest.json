{
  "manifest_version": 2,
  "name": "UDEX",
  "description": "A quality-of-life Extension for Udacity Reviewers.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "UDEX Chrome Extension"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://review.udacity.com/*"
      ],
      "js": [
        "src/build/main.js"
      ]
    }
  ]
}